{
  "$schema": "https://json.nx.dev/project-schema.json",
  "name": "cliq-frontend",
  "sourceRoot": "apps/cliq-frontend",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/cliq-frontend",
        "commands": ["pnpm build"]
      }
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "apps/cliq-frontend",
        "commands": ["pnpm dev"],
        "parallel": false
      }
    }
  }
}